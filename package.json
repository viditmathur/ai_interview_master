{
  "name": "ai-interview-master",
  "version": "1.0.0",
  "description": "AI-powered interview platform with resume analysis and real-time evaluation",
  "main": "server/index.ts",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd client && npm run build",
    "build:server": "tsc server/index.ts --outDir dist/server --target es2020 --module commonjs",
    "start": "NODE_ENV=production node dist/server/index.js",
    "vercel-build": "npm run build",
    "db:push": "npx drizzle-kit push",
    "db:generate": "npx drizzle-kit generate",
    "db:migrate": "npx drizzle-kit migrate",
    "install-deps": "npm install --legacy-peer-deps"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "openai": "^4.20.1",
    "drizzle-orm": "^0.44.3",
    "postgres": "^3.4.3",
    "nanoid": "^5.0.4",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "@sendgrid/mail": "^8.1.1",
    "agora-rtc-sdk-ng": "^4.20.0"
  },
  "devDependencies": {
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "drizzle-kit": "^0.20.9",
    "@types/cors": "^2.8.17",
    "@types/multer": "^1.4.11",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "@types/react": "^18.2.0"
  }
}
